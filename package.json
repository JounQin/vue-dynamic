{
  "name": "vue-dynamic",
  "version": "0.1.0",
  "description": "Load stringified or normal Vue components dynamically!",
  "keywords": [
    "vue",
    "dynamic",
    "component",
    "dynamic components"
  ],
  "main": "dist/index.js",
  "module": "lib/index.js",
  "unpkg": "dist/vue-dynamic.js",
  "files": [
    "dist",
    "lib"
  ],
  "repository": {
    "url": "https://github.com/JounQin/vue-dynamic",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/JounQin/vue-dynamic/issues",
    "email": "admin@1stg.me"
  },
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rollup -c && cross-env ENTRY=dynamic rollup -c",
    "dist": "npm run build && cross-env NODE_ENV=production npm run build",
    "lint": "eslint . --max-warnings=10"
  },
  "eslintConfig": {
    "extends": "1stg"
  },
  "eslintIgnore": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings=10",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.0.0",
    "eslint-config-1stg": "^2.1.0",
    "husky": "^1.0.0-rc.9",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.5",
    "rollup": "^0.61.2",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-uglify": "^4.0.0",
    "vue": "^2.5.16"
  }
}
