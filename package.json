{
  "name": "vue-dynamic",
  "version": "0.0.6",
  "description": "Load stringified or normal Vue components dynamically!",
  "keywords": [
    "vue",
    "dynamic",
    "component",
    "dynamic components"
  ],
  "main": "index.js",
  "unpkg": "dist/vue-dynamic.js",
  "files": [
    "index.js",
    "dynamic.js",
    "dist/*.js"
  ],
  "repository": {
    "url": "https://github.com/JounQin/vue-dynamic",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/JounQin/vue-dynamic/issues",
    "email": "admin@1stg.me"
  },
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "build": "node webpack",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "dist": "cross-env DEBUG=vue-dynamic npm run build && npm run build:prod",
    "lint": "eslint . --max-warnings=10",
    "lint:fix": "yarn lint --fix"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.0",
    "debug": "^3.1.0",
    "eslint": "^4.8.0",
    "eslint-config-1stg": "^0.0.5",
    "lodash": "^4.17.4",
    "vue": "^2.3.3",
    "webpack": "^4.0.0"
  }
}
