{
  "name": "vue-dynamic",
  "version": "0.3.1",
  "description": "Load stringified or normal Vue components dynamically!",
  "keywords": [
    "vue",
    "dynamic",
    "component",
    "dynamic components"
  ],
  "main": "dist/vue-dynamic.js",
  "module": "esm/index.js",
  "unpkg": "dist/vue-dynamic.js",
  "files": [
    "dist",
    "esm"
  ],
  "repository": {
    "url": "https://github.com/JounQin/vue-dynamic",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/JounQin/vue-dynamic/issues",
    "email": "admin@1stg.me"
  },
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rollup -c && cross-env NODE_ENV=production rollup -c",
    "esm": "buble -i lib --yes dangerousForOf --no modules --objectAssign -o esm",
    "lint": "eslint . --max-warnings=10"
  },
  "eslintConfig": {
    "extends": "1stg"
  },
  "eslintIgnore": [
    "dist",
    "esm"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings=10 --fix",
      "git add"
    ],
    "*.{html,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-config-1stg": "^4.1.0",
    "husky": "^3.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "rollup": "^1.1.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.1",
    "vue": "^2.5.21"
  }
}
